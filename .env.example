PORT=3000

# MongoDB
# Example (MongoDB Atlas):
# mongodb+srv://<user>:<password>@<cluster>.mongodb.net/?retryWrites=true&w=majority&appName=<appName>
MONGODB_URI=

# Auth
# Use a long, random string in production
JWT_SECRET=
JWT_EXPIRE=24h

# Python Model API
# Required. No trailing slash.
# Local dev (if running model locally): http://localhost:7860
# Hosted (Hugging Face Space or similar): https://santacl-prometheus.hf.space
PYTHON_API_URL=

# Node environment: development | production
NODE_ENV=development
